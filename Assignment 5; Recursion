import java.util.*;
public class Challenge {

public static int x = 0;
	static Scanner input = new Scanner(System.in);
	static void p(int y){
		System.out.println("The next number is: " + y);
		y=y+10;
		p(y);
	}
	
	public static void main(String[] args) {
		System.out.println("Please enter the number you would like to increment by 10.");
		int y = input.nextInt();
		p(y);
	}
}
